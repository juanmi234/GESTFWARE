@page
@model FrontEnd.Pages.CArbitro.DetailsModel
@{
    ViewData["Title"]="Detalles Arbitros";
}

<h2>Detalles Arbitro</h2>
<hr/>
<div class="row">
    <div class="col-md-4">
        <form method="get" asp-action="Details">
            <div class="form-group">
                <label asp-for="arbitro.Documento" class="control-label"></label>
                <input asp-for="arbitro.Documento" class="control-label" readonly/>
            </div>
            <br>
            <div class="form-group">
                <label asp-for="arbitro.Nombres" class="control-label"></label>
                <input asp-for="arbitro.Nombres" class="form-control" readonly/>
            </div>
            <br>
            <div class="form-group">
                <label asp-for="arbitro.Apellidos" class="control-label"></label>
                <input asp-for="arbitro.Apellidos" class="form-control" readonly/>
            </div>
            <br>
            <div class="form-group">
                <label asp-for="arbitro.Deporte" class="control-label"></label>
                <input asp-for="arbitro.Deporte" class="form-control" readonly/>
            </div>
            <br>
            <div class="form-group">
                <label asp-for="arbitro.Rh" class="control-label"></label>
                <input asp-for="arbitro.Rh" class="form-control" readonly/>
            </div>
            <br>
            <div class="form-group">
                <label asp-for="arbitro.Celular" class="control-label"></label>
                <input asp-for="arbitro.Celular" class="form-control" readonly/>
            </div>
            <br>
            <div class="form-group">
                <label asp-for="arbitro.Colegio.RazonSocial" class="control-label"></label>
                <input asp-for="arbitro.Colegio.RazonSocial" class="form-control" readonly/>
            </div>
            <br>
            <div class="form-group">
                <label asp-for="arbitro.Torneo.Nombre" class="control-label"></label>
                <input asp-for="arbitro.Torneo.Nombre" class="form-control" readonly/>
            </div>
            <br>
            <div class="form-group">
                <a asp-page="./Index" class="btn btn-info">Regresar</a>
            </div>
        </form>
    </div>

</div>
